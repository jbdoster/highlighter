{
	"name": "highlighter",
	"displayName": "highlighter",
	"description": "Highlight, name, and save locations in your code files.",
	"version": "1.0.7",
	"publisher": "xlntdevelopment",
	"author": {
		"name": "Jonathan Doster",
		"email": "jonathan@whiskerandpaw.co",
		"url": "https://www.linkedin.com/in/jbdoster/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jbdoster/highlighter.git"
	},
	"keywords": [
		"highlighter",
		"xlntdevelopment",
		"marker",
		"location",
		"organization",
		"organize",
		"mark",
		"tag"
	],
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:extension.highlightSelection",
		"onCommand:extension.findHighlight",
		"onCommand:extension.removeHighlight",
		"onCommand:extension.removeAllHighlights"
	],
	"main": "./dist/src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.highlightSelection",
				"title": "Highlight Selection"
			},
			{
				"command": "extension.findHighlight",
				"title": "Find Highlighted Line(s)"
			},
			{
				"command": "extension.removeHighlight",
				"title": "Remove Highlighted Line(s)"
			},
			{
				"command": "extension.removeAllHighlights",
				"title": "Remove All Saved Highlights"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "mocha -r ts-node/register test/**/*.spec.ts"
	},
	"devDependencies": {
		"@types/chai": "^4.2.11",
		"@types/mocha": "^2.2.48",
		"@types/node": "^10.12.21",
		"ts-loader": "^6.2.2",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@types/jsonfile": "^5.0.0",
		"chai": "^4.2.0",
		"jsonfile": "^5.0.0",
		"ts-node": "^8.6.2"
	},
	"icon": "extension/resources/imgs/logo.png"
}
