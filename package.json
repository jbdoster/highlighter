{
	"name": "highlighter",
	"displayName": "highlighter",
	"description": "Highlight, name, and save locations in your code files.",
	"version": "1.0.2",
	"publisher": "xlntdevelopment",
	"author": {
		"name": "Jonathan Doster",
		"email": "jonathan@whiskerandpaw.co",
		"url": "https://www.linkedin.com/in/jbdoster/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jbdoster/highlighter.git"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.highlightLines",
		"onCommand:extension.highlightFolder",
		"onCommand:extension.findHighlight",
		"onCommand:extension.removeHighlight",
		"onCommand:extension.removeAllHighlights"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.highlightLines",
				"title": "Highlight Line(s)"
			},
			{
				"command": "extension.highlightFolder",
				"title": "Highlight Folder(s)"
			},
			{
				"command": "extension.findHighlight",
				"title": "Find Highlighted Line(s)"
			},
			{
				"command": "extension.removeHighlight",
				"title": "Remove Highlighted Line(s)"
			},
			{
				"command": "extension.removeAllHighlights",
				"title": "Remove All Saved Highlights"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile && node ./node_modules/vscode/bin/test",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"@types/jsonfile": "^5.0.0",
		"jsonfile": "^5.0.0"
	},
	"icon": "src/resources/imgs/logo.png"
}
